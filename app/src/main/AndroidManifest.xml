<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.makeyourcs">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".FirebaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".Signup_profileActivity" />
        <activity android:name=".ui.signup.Signup_celeActivity" />
        <activity android:name=".ui.signup.Signup_bdayActivity" />
        <activity android:name=".ui.signup.Signup_emailActivity" />
        <activity android:name=".ui.signup.Signup_pwActivity" />
        <activity android:name=".ui.signup.Signup_idActivity" />
        <activity android:name=".ui.auth.SignupActivity"/>
        <activity android:name=".ui.home.HomeActivity" />

    </application>

</manifest>